
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="../../mymaps/js/lib/d3.js"></script>
<script src="../../mymaps/js/lib/klass.js"></script>
<script src="../../mymaps/js/atlaspr.js"></script>


<style>
    .carousel-inner > .item > img,
    .carousel-inner > .item > a > img {
        width: 70%;
        margin: auto;
    }
</style>


<!--tumbnail-->

<div id="map"></div>

<script type='text/javascript'>
    var my_node = document.getElementById("map");
    new AtlasPR({
        node: my_node
    });
</script>

<div class="col-md-4">

    <div class="row">
        <div class="thumbnail">
            <h3><b>Today's Recommendations</b></h3>
            <!--Carousel Begin-->
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>

                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner" role="listbox">
                    <div class="item active">
                        <img src="../../assets/admin/pages/media/gallery/image3.jpg" class="img-responsive" alt="Rincon" width="460" height="345">
                        <div class="carousel-caption">
                            <h3>Rincón</h3>
                            <p>Enjoy of this beatufil beach after a DB exam!</p>
                        </div>
                    </div>

                    <div class="item">
                        <img src="../../assets/admin/pages/media/gallery/image4.jpg" class="img-responsive" alt="Yunque" width="460" height="345">
                        <div class="carousel-caption">
                            <h3>Yunque</h3>
                            <p>Get lost in "El Yunque" if your DB project does'nt run!</p>
                        </div>
                    </div>

                    <div class="item">
                        <img src="../../assets/admin/pages/media/gallery/image5.jpg" class="img-responsive" alt="Morro" width="460" height="345">
                        <div class="carousel-caption">
                            <h3>Morro</h3>
                            <p>Jump from "El Morro" after watching your grade!</p>
                        </div>
                    </div>


                </div>

                <!-- Left and right controls -->
                <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
            <!--End Carrousel-->
            <div class="caption">

                <p>Know more about this three recommended places!</p>
                <p><a href="#" class="btn btn-primary" role="button">Favorites ♥ </a><a href="#" class="btn btn-default" role="button">Wish List ♣</a></p>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="thumbnail">
            <h3><b>Future events</b></h3>
            <div class="list-group">
                <a href="#" class="list-group-item active">
                    <h4 class="list-group-item-heading">Pool Party | July 7,2016</h4>
                    <p class="list-group-item-text">Enjoy of the best food & drinks!</p>
                    <span class="glyphicon glyphicon-calendar" aria-hidden="true"></span>
                </a>

                <a href="#" class="list-group-item">
                    <h4 class="list-group-item-heading">Salsa Night | Nov 23,2015</h4>
                    <p class="list-group-item-text">Learn how to dance!</p>
